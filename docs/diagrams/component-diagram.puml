@startuml
!theme plain
skinparam backgroundColor white

package "Frontend" {
  component [App] as App
  component [Dashboard] as Dashboard
  component [Tasks] as Tasks
  component [Projects] as Projects
}

package "Backend" {
  component [Supabase] as Supabase
}

package "Database" {
  database [PostgreSQL] as DB
}

App --> Dashboard
App --> Tasks
App --> Projects
Dashboard --> Supabase
Tasks --> Supabase
Projects --> Supabase
Supabase --> DB

@enduml